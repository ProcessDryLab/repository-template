<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.web>
		<!-- 100 MB in kilobytes -->
		<httpRuntime maxRequestLength="204800" />
	</system.web>

	<system.webServer>
		<security>
			<requestFiltering>
				<!-- 100 MB in bytes -->
				<requestLimits maxAllowedContentLength="209715200" />
			</requestFiltering>
		</security>
	</system.webServer>
</configuration>
